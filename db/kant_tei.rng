                     <ref name="tei_model.global"/>
                  </zeroOrMore>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <zeroOrMore>
                        <ref name="tei_model.global"/>
                     </zeroOrMore>
                     <ref name="tei_item"/>
                     <zeroOrMore>
                        <ref name="tei_model.global"/>
                     </zeroOrMore>
                  </oneOrMore>
               </group>
            </choice>
            <zeroOrMore>
               <ref name="tei_model.divBottom"/>
